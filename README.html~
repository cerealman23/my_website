          (use-package org
            <span style="font-weight: bold;">:ensure</span> t
              <span style="font-weight: bold;">:hook</span> (org-mode . dw/org-mode-setup)
            <span style="font-weight: bold;">:config</span>
            (<span style="font-weight: bold;">setq</span> org-ellipsis <span style="font-style: italic;">" &#9662;"</span>
                  org-hide-emphasis-markers t)

            (<span style="font-weight: bold;">setq</span> org-agenda-files
        '(<span style="font-style: italic;">"/mnt/c/Home/OrgAgenda/tasks.org"</span>
          <span style="font-style: italic;">"/mnt/c/Home/OrgAgenda/birthdays.org"</span>
          <span style="font-style: italic;">"/mnt/c/Home/OrgAgenda/Homework.org"</span>
          <span style="font-style: italic;">"/mnt/c/Home/OrgAgenda/Events.org"</span>))
)


