(use-package ccls
  <span style="font-weight: bold;">:ensure</span> t
  <span style="font-weight: bold;">:config</span>
  (<span style="font-weight: bold;">setq</span> ccls-executable <span style="font-style: italic;">"ccls"</span>)
  (<span style="font-weight: bold;">setq</span> lsp-prefer-flymake nil)
  (<span style="font-weight: bold;">setq-default</span> flycheck-disabled-checkers '(c/c++-clang c/c++-cppcheck c/c++-gcc))
  <span style="font-weight: bold;">:hook</span> ((c-mode c++-mode objc-mode) .
         (<span style="font-weight: bold;">lambda</span> () (<span style="font-weight: bold;">require</span> '<span style="font-weight: bold; text-decoration: underline;">ccls</span>) (lsp))))

